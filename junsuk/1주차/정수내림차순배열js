function solution(n) {
    const arr1 = Array.from(String(n));
    const arr2 = [];
    
    for(let i = 0; i < n.toString().length; i+=1){
        let big = 0;
        for(const item of arr1){
            if(item > big) big = item;
        }
        arr2.push(big);
        console.log(arr1);
        console.log(big)
        const idx = arr1.lastIndexOf(big);
        arr1.splice(idx, 1);
        
    }
    return +arr2.join("");
}
